#start

#input
inp1 = int(input("plz enter your first num"))
inp2 = int(input("plz enter your second num"))

#lambda
m = lambda a, b : a - b
t = lambda a, b : a / b
j = lambda a, b : a + b
z = lambda a, b : a * b

#print
print("menha :")
print(z(inp1, inp2))
print("taghsim :")
print(j( inp1, inp2))
print("jam :")
print(t(inp1, inp2))
print("zarb :")
print(m( inp1, inp2))

#end
