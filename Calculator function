inp1 = int(input("plz enter your first num"))
inp2 = int(input("plz enter your sec num"))


def zarb(z):
  print(z, inp1 * inp2)
  
zarb("zarb :")

def jam(j):
  print(j, inp1 + inp2)

jam("jam :")

def menha(m):
  print(m, inp1 - inp2)

menha("menha :")

def taghsim(t):
  print(t, inp1 / inp2)

taghsim("taghsim :")
